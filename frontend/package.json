  "scripts": {
    "lint": "eslint --ext .tsx,.ts src/",
    "typecheck": "tsc --noemit",
    "build": "gatsby build",
    "build:cy": "CYPRESS_SUPPORT=y gatsby build",
    "develop": "CYPRESS_SUPPORT=y gatsby develop -H 0.0.0.0 -p 8001",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "storybook": "NODE_ENV=production start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook",
    "gen:component": "hygen component new --name",
    "gen:graphql-types": "graphql-codegen --config codegen.yml",
    "test": "jest --coverage --passWithNoTests --coverageReporters json clover",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:ci": "start-server-and-test develop 8001 cy:run"
